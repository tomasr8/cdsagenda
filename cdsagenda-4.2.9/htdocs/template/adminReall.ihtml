<html>
  <head>
    <title>CDS Agendas Administrator</title>
    <link rel="stylesheet" href="../images/style.css">
  </head>
  <body class=admin>
    <table align="center" width="100%" bgcolor="#CCCCCC">
      <tr>
        <td align="center"><img src="{img_reall}" align="middle">&nbsp;<b>Re-allocating {category_name}</b></td>
      </tr>
    </table>
    <br>
    <table width="90%" align="center" bgcolor="#339966">
      <tr>
        <td align="left"><font size="2"><b>COMMON ACTIONS</b></font></td>
      </tr>
      <tr>
        <td bgcolor="white">{common_actions}</td>
      </tr>
    </table>
    <br>
    <table width="90%" align="center" bgcolor="#339966">
      <tr>
        <td align="left"><font size="2"><b>MANUAL SELECTION</b></font></td>
      </tr>
      <tr>
        <td bgcolor="white">{manual_selection}</td>
      </tr>
    </table>
    <br>
    <table bgcolor="#CCCCCC" align="center" width="100%">
      <tr>
        <td align="center">&nbsp;</td>
      </tr>
    </table>
  </body>
</html>
